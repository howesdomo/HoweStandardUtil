<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>Util.HowesDOMO</PackageId>
    <Authors>HowesDOMO</Authors>
    <Company>HowesDOMOHowesDOMO</Company>
    <Product>Util.HowesDOMO</Product>
    <PackageTags>dotNet_Standard</PackageTags>
