  <ItemGroup>
    <Folder Include="Excel\" />
    <Folder Include="Json\JsonConverters\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
